{
  "name": "knockr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn workspace @knockr/mui build:rollup",
    "build:mui": "yarn workspace @knockr/mui prepublish",
    "build:storybook": "yarn workspace @knockr/mui build:storybook",
    "dev": "yarn workspace @knockr/mui dev",
    "dev:local": "yarn workspace @knockr/mui dev:local",
    "dev:preview": "yarn workspace @knockr/mui preview",
    "preview": "yarn workspace @knockr/mui preview",
    "www": "serve www -l 1234",
    "publish:mui": "yarn workspace @knockr/mui publish --access=public",
    "publish:client": "yarn workspace @knockr/client publish --access=public",
    "lint": "yarn workspace @knockr/mui tsc"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@sentry/browser": "^6.19.6",
    "@sentry/minimal": "^6.19.6",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@storybook/theming": "^6.4.21",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.165",
    "@types/react-dom": "^18.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "rollup": "^2.70.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^13.0.2",
    "typescript": "^4.6.3"
  }
}
